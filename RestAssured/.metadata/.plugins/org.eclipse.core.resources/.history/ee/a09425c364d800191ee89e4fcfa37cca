package login.io.login;

import org.testng.Assert;
import org.hamcrest.*;
import org.testng.annotations.Test;

import io.restassured.RestAssured;
import io.restassured.response.Response;

public class google {

	private static final int HTTP_STATUS_CODE = 200;

	@Test
	public void testgoogle() {

		Response resp = RestAssured.get("http://www.google.com");
		Assert.assertEquals(HTTP_STATUS_CODE, resp.getStatusCode());

		RestAssured.post();

		System.err.println("Session ID: " + resp.getSessionId());
		System.err.println("Status Line: " + resp.getStatusLine());
		System.err.println("Time: " + resp.getTime());
		System.err.println("Response Body : " + resp.getBody());
		System.out.println(resp.asString());

	}

}
