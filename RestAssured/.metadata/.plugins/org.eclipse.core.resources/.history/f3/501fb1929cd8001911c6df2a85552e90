
public class HashCodeGenerator {
	public static void main(String[] args) {
		String name = "JITE";
		System.out.println("name: " + name + " & hashcode is : " + name.hashCode());
		
		
		char []  array = name.toCharArray();
		for(int i=0;i<array.length;i++)
		{
			if(array[i]== '\0')
			{
				throw new Exception("");
			}
		}
	}
}
